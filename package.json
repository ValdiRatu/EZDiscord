{
  "name": "ezdiscord",
  "license": "GPL-1.0+",
  "scripts": {
    "ezd-compile": "rd /S /Q out 2>nul & mkdir out 2>nul & ts-node src/main/GenerateFiles.ts",
    "ezd-run": "npx ts-node src/main/RunBot.ts",
    "ezd-start": "yarn ezd-compile && yarn ezd-run",
    "antlr": "antlr4ts -visitor src/parser/EZDiscordLexer.g4 src/parser/EZDiscordParser.g4 -o src/parser -Xexact-output-dir"
  },
  "dependencies": {
    "@discordjs/rest": "^1.3.0",
    "antlr4ts": "^0.5.0-alpha.4",
    "chalk": "4.1.2",
    "discord.js": "^14.5.0",
    "dotenv": "^16.0.3",
    "ts-morph": "^16.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.8.1",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
